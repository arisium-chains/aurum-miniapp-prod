{
  "name": "@apps/ml-api",
  "version": "1.0.0",
  "description": "Node.js ML Face Scoring API with ONNX.js - Standardized Monorepo Version",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "docker:build": "docker build -t ml-api .",
    "docker:run": "docker run -p 3000:3000 ml-api"
  },
  "keywords": [
    "ml",
    "face-scoring",
    "nodejs",
    "onnx",
    "api",
    "monorepo"
  ],
  "author": "Aurum Circle",
  "license": "MIT",
  "dependencies": {
    "@shared/types": "workspace:*",
    "@shared/utils": "workspace:*",
    "@shared/config": "workspace:*",
    "@bull-board/express": "^5.15.0",
    "@bull-board/ui": "^5.15.0",
    "bullmq": "^5.7.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "joi": "^17.12.3",
    "multer": "^1.4.5-lts.1",
    "onnxruntime-node": "^1.17.1",
    "sharp": "^0.33.3",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
