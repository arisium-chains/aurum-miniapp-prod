{
  "name": "ml-face-score-api",
  "version": "1.0.0",
  "description": "Node.js ML Face Scoring API with ONNX.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
